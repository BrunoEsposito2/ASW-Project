      "name": "Stanimira Vlaeva",
      "url": "https://github.com/sis0k0"
    }
  ],
  "license": "Apache 2.0",
  "homepage": "https://github.com/mongodb-developer/mean-stack-example#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb-developer/mean-stack-example.git"
  },
  "devDependencies": {
    "@types/bootstrap": "^5.2.8",
    "@types/cors": "^2.8.13",
    "@types/events": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/jquery": "^3.5.25",
    "@types/node": "^20.4.2",
    "concurrently": "^7.6.0",
    "jquery": "^3.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@angular/cdk": "^16.1.0",
    "@angular/material": "^16.1.0",
    "@ng-bootstrap/ng-bootstrap": "^15.1.2",
    "bootstrap": "^5.3.2",
    "chart.js": "^4.3.0",
    "core-js": "^3.23.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "lru-cache": "^10.0.0",
    "mongodb": "^5.7.0",
    "socket.io-client": "^4.6.1",
    "types": "^0.1.1"
  }
